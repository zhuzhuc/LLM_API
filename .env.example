# 数据库配置
DATABASE_URL=sqlite3://./llm.db

# JWT 配置
JWT_SECRET=your-secret-key-change-in-production

# 环境配置
ENVIRONMENT=development

# Token 配置
TOKEN_RATE=0.001
DEFAULT_TOKENS=1000

# 服务器配置
SERVER_PORT=8080
LLAMA_CPP_PORT=8081

# llama.cpp 配置
LLAMA_CPP_PATH=./llama-cpp/build/bin/llama-server
MODELS_PATH=./models
MODEL_CONFIG_PATH=./models/model_config.json

# Keycloak 配置 (可选)
KEYCLOAK_URL=
KEYCLOAK_REALM=
KEYCLOAK_CLIENT_ID=
